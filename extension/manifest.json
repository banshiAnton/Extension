{
    "manifest_version": 2,
    "name": "HLTV Live Matches",
    "description": "HLTV Matches",
    "version": "1.0",

"browser_action": {
    "default_popup": "index.html"  
    },

"background": {
    "scripts": 
      ["socketIO.js", "howl.js", "background.js"]
  },

  "permissions" : ["notifications", "tabs", "storage"],	
  "icons": { "16": "logo.png",
               "48": "logo.png",
               "128": "logo.png" }	   
}